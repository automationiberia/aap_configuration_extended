                                   | default(template_overrides_global.eda_rulebook_activation.project_name)
                                   | default(query_project_name) }}"
{% endif %}
{% if template_overrides_resources.eda_rulebook_activation[current_rulebook_activations_asset_value.name].decision_environment is defined
      or template_overrides_global.eda_rulebook_activation.decision_environment is defined
      or query_decision_environment_name is defined %}
