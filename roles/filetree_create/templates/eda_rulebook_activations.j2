{% if template_overrides_resources.eda_rulebook_activation[current_rulebook_activations_asset_value.name].is_enabled is defined
      or template_overrides_global.eda_rulebook_activation.is_enabled is defined
      or current_rulebook_activations_asset_value.is_enabled %}
