{% endif %}
{% if template_overrides_resources.eda_rulebook_activation[current_rulebook_activations_asset_value.name].organization_name is defined
      or template_overrides_global.eda_rulebook_activation.organization_name is defined
      or query_organization_name is defined %}
    organization_name: "{{ template_overrides_resources.eda_rulebook_activation[current_rulebook_activations_asset_value.name].organization_name
