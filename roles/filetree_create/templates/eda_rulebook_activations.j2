{% if (current_rulebook_activation_index | default(0)) == 0 %}
---
eda_rulebook_activations:
{% endif %}
  - name: "{{ current_rulebook_activations_asset_value.name }}"
    description: "{{ template_overrides_resources.eda_rulebook_activation[current_rulebook_activations_asset_value.name].description
                     | default(template_overrides_global.eda_rulebook_activation.description)
                     | default(current_rulebook_activations_asset_value.description) }}"
{% if template_overrides_resources.eda_rulebook_activation[current_rulebook_activations_asset_value.name].is_enabled is defined
      or template_overrides_global.eda_rulebook_activation.is_enabled is defined
      or current_rulebook_activations_asset_value.is_enabled %}
    is_enabled: "{{ template_overrides_resources.eda_rulebook_activation[current_rulebook_activations_asset_value.name].is_enabled
                    | default(template_overrides_global.eda_rulebook_activation.is_enabled)
                    | default(current_rulebook_activations_asset_value.is_enabled) }}"
{% endif %}
{% if template_overrides_resources.eda_rulebook_activation[current_rulebook_activations_asset_value.name].status is defined
      or template_overrides_global.eda_status is defined
      or current_rulebook_activations_asset_value.status is defined %}
    status: "{{ template_overrides_resources.eda_rulebook_activation[current_rulebook_activations_asset_value.name].status
                | default(template_overrides_global.eda_status)
                | default(current_rulebook_activations_asset_value.status) }}"
{% endif %}
{% if template_overrides_resources.eda_rulebook_activation[current_rulebook_activations_asset_value.name].git_hash is defined
      or template_overrides_global.git_hash is defined
      or current_rulebook_activations_asset_value.git_hash is defined %}
    scm_branch: "{{ template_overrides_resources.eda_rulebook_activation[current_rulebook_activations_asset_value.name].git_hash
                    | default(template_overrides_global.eda_rulebook_activation.git_hash)
                    | default(current_rulebook_activations_asset_value.git_hash) }}"
{% endif %}
    scm_refspec: "{{ template_overrides_resources.eda_rulebook_activation[current_rulebook_activations_asset_value.name].scm_refspec
                     | default(template_overrides_global.eda_rulebook_activation.scm_refspec)
                     | default(current_rulebook_activations_asset_value.scm_refspec) }}"
{% if template_overrides_resources.eda_rulebook_activation[current_rulebook_activations_asset_value.name].default_environment is defined
      or template_overrides_global.eda_rulebook_activation.default_environment is defined
      or current_job_templates_asset_value.summary_fields.default_environment is defined %}
    default_environment: "{{ template_overrides_resources.eda_rulebook_activation[current_rulebook_activations_asset_value.name].default_environment
                             | default(template_overrides_global.eda_rulebook_activation.default_environment)
                             | default(current_job_templates_asset_value.summary_fields.default_environment.name) }}"
{% endif %}
{% if last_rulebook_activation | default(true) | bool %}
...
{% endif %}
